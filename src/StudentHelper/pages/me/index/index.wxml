<view class="container">
  <view id="header" bindtap="information">
    <image id="avatarUrl" src="{{avatarUrl}}"></image>
    <view class="userInfo">
      <text id="nickName">{{nickName}}</text>
      <!-- <text id="signature">这个人很懒，什么都没有留下...</text> -->
    </view>
  </view>

  <view class="bg"></view>
  <view id="operateItems">
    <navigator url="/pages/me/study_list/study_list?code=0">
      <view class="item">
        <image src="/images/me-1.png"></image>
        <text>我的动态</text>
      </view>
    </navigator>
    <navigator url="/pages/me/myJobs/myJobs">
      <view class="item">
        <image src="/images/me-2.png"></image>
        <text>我的兼职</text>
      </view>
    </navigator>
    <navigator url="/pages/me/study_list/study_list?code=1">
      <view class="item">
        <image src="/images/me-3.png"></image>
        <text>我的收藏</text>
      </view>
    </navigator>
    <view class="bg"></view>
    <navigator url="/pages/me/feedback/feedback" class="feedback">
      <view class="item">
        <image src="/images/job.png"></image>
        <text>意见反馈</text>
      </view>
    </navigator>
    <view class="item about" catchtap="abouts">
      <image src="/images/about.png"></image>
      <text>关于我们</text>
    </view>
  </view>
</view>